resharper_place_simple_initializer_on_single_line = false
resharper_remove_blank_lines_near_braces_in_code = false
resharper_resx_max_line_length = 170
resharper_shaderlab_max_line_length = 170
resharper_show_autodetect_configure_formatting_tip = false
resharper_simple_case_statement_style = on_single_line
resharper_space_within_initializer_braces = true
resharper_T4_max_line_length = 170
resharper_toplevel_function_declaration_return_type_style = on_single_line
