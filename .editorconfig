resharper_indent_switch_labels = true
resharper_instance_members_qualify_declared_in = 
resharper_int_align_comments = true
resharper_keep_existing_arrangement = true
resharper_max_array_initializer_elements_on_line = 2
resharper_max_attribute_length_for_same_line = 10000
resharper_new_line_before_while = true
resharper_object_creation_when_type_evident = explicitly_typed
resharper_parentheses_non_obvious_operations = none, shift, bitwise_and, bitwise_exclusive_or, bitwise_inclusive_or, bitwise, conditional_and, conditional_or, conditional
resharper_parentheses_redundancy_style = remove
